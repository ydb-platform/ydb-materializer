  "panels": [
    {
      "type": "timeseries",
      "title": "CDC Read Rate",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (consumer, topic) (rate(ydbmv_cdc_records_read_total{consumer=~\"$consumer\",topic=~\"$topic\"}[5m]))",
          "legendFormat": "{{consumer}} {{topic}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CDC Submitted Rate",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (consumer, topic) (rate(ydbmv_cdc_records_submitted_total{consumer=~\"$consumer\",topic=~\"$topic\"}[5m]))",
          "legendFormat": "{{consumer}} {{topic}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CDC Parse Errors Rate",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 7, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (consumer, topic) (rate(ydbmv_cdc_parse_errors_total{consumer=~\"$consumer\",topic=~\"$topic\"}[5m]))",
          "legendFormat": "{{consumer}} {{topic}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CDC Parse p95",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 7, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, consumer, topic) (rate(ydbmv_cdc_parse_seconds_bucket{consumer=~\"$consumer\",topic=~\"$topic\"}[5m])))",
          "legendFormat": "{{consumer}} {{topic}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CDC Submit p95",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 14, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, consumer, topic) (rate(ydbmv_cdc_submit_seconds_bucket{consumer=~\"$consumer\",topic=~\"$topic\"}[5m])))",
          "legendFormat": "{{consumer}} {{topic}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "MV Processed Rate",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 14, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (type, target, alias, source, item, action) (rate(ydbmv_mv_records_processed_total{type=~\"$type\",target=~\"$target\",alias=~\"$alias\",source=~\"$source\",item=~\"$item\",action=~\"$action\"}[5m]))",
          "legendFormat": "{{type}} {{target}} {{alias}} {{source}} {{item}} {{action}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "MV Processing Errors Rate",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 21, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (type, target, alias, source, item, action) (rate(ydbmv_mv_processing_errors_total{type=~\"$type\",target=~\"$target\",alias=~\"$alias\",source=~\"$source\",item=~\"$item\",action=~\"$action\"}[5m]))",
          "legendFormat": "{{type}} {{target}} {{alias}} {{source}} {{item}} {{action}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "MV Processing p95",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 21, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, type, target, alias, source, item, action) (rate(ydbmv_mv_processing_seconds_bucket{type=~\"$type\",target=~\"$target\",alias=~\"$alias\",source=~\"$source\",item=~\"$item\",action=~\"$action\"}[5m])))",
          "legendFormat": "{{type}} {{target}} {{alias}} {{source}} {{item}} {{action}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "SQL p95 by Operation",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 28, "w": 24, "h": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, type, target, alias, action) (rate(ydbmv_mv_sql_seconds_bucket{type=~\"$type\",target=~\"$target\",alias=~\"$alias\",action=~\"$sql_action\"}[5m])))",
          "legendFormat": "{{type}} {{target}} {{alias}} {{action}}"
        }
      ]
    }
