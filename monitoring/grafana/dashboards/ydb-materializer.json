{
  "id": null,
  "uid": "ydb-materializer",
  "title": "YDB Materializer",
  "tags": ["ydb", "materializer"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "templating": {
